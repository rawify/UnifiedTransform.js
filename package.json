{
  "name": "unifiedtransform",
  "title": "UnifiedTransform",
  "version": "0.4.0",
  "homepage": "https://github.com/rawify/UnifiedTransform.js",
  "bugs": {
    "url": "https://github.com/rawify/UnifiedTransform.js/issues"
  },
  "description": "The RAW library to work with CSS transform strings",
  "keywords": [
    "css",
    "transform",
    "matrix",
    "math",
    "translate",
    "rotate",
    "scale",
    "2d"
  ],
  "private": false,
  "main": "./dist/unifiedtransform.js",
  "module": "./dist/unifiedtransform.mjs",
  "types": "./unifiedtransform.d.ts",
  "browser": "./dist/unifiedtransform.min.js",
  "unpkg": "./dist/unifiedtransform.min.js",
  "readmeFilename": "README.md",
  "exports": {
    ".": {
      "types": "./unifiedtransform.d.ts",
      "require": "./dist/unifiedtransform.js",
      "import": "./dist/unifiedtransform.mjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rawify/UnifiedTransform.js.git"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/rawify"
  },
  "author": {
    "name": "Robert Eisele",
    "email": "robert@raw.org",
    "url": "https://raw.org/"
  },
  "license": "MIT",
  "engines": {
    "node": "*"
  },
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "mocha tests/*.js",
    "build": "crude-build UnifiedTransform"
  },
  "devDependencies": {
    "crude-build": "^0.1.2",
    "mocha": "*"
  }
}
